{
	"name" : "raincity/redcap-library",
	"description" : "Library to support working with REDCap",
	"type" : "library",
	"version" : "2.0.8",
	"license" : "BSD-3-Clause",
	"minimum-stability" : "stable",
	"repositories" : [{
			"type" : "git",
			"url" : "https://github.com/RainCitySolutions/php-library",
			"name" : "php-ibrary"
		}
	],
	"require" : {
		"php" : ">=8.2",
		"composer/installers" : "~2.2",
		"iu-redcap/phpcap" : "~1.4",
		"raincity/php-library" : "~2.0"
	},
	"autoload" : {
		"classmap" : [
			"src/"
		],
		"psr-4" : {
			"" : [
				"src",
				"test"
			]
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "~9.6",
		"brain/monkey" : "~2.6"
	},
	"config" : {
		"allow-plugins" : {
			"composer/installers" : true
		}
	}
}